{
  "kind": "collectionType",
  "collectionName": "blogs",
  "info": {
    "singularName": "blog",
    "pluralName": "blogs",
    "displayName": "Blog",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "BlogId": {
      "type": "uid",
      "required": true
    },
    "topic": {
      "type": "string",
      "required": true,
      "unique": false
    },
    "content": {
      "type": "text",
      "required": true
    },
    "title": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "imageURL": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "keywords": {
      "type": "json"
    },
    "viewCount": {
      "type": "biginteger",
      "required": true,
      "default": "01"
    },
    "day": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::day.day",
      "mappedBy": "blog"
    }
  }
}
