{
  "kind": "collectionType",
  "collectionName": "up_users",
  "info": {
    "name": "user",
    "description": "",
    "singularName": "user",
    "pluralName": "users",
    "displayName": "User"
  },
  "options": {
    "draftAndPublish": false
  },
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": true
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmationToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "role": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.role",
      "inversedBy": "users",
      "configurable": false
    },
    "age": {
      "type": "integer",
      "required": true
    },
    "phone": {
      "type": "biginteger",
      "required": true,
      "unique": true
    },
    "compliteDay": {
      "type": "integer",
      "default": 0
    },
    "country": {
      "type": "string",
      "required": true,
      "default": "Bangladesh"
    },
    "videoComplete": {
      "type": "boolean",
      "default": false
    },
    "kagelComplete": {
      "type": "boolean",
      "default": false
    },
    "quizComplete": {
      "type": "boolean",
      "default": false
    },
    "sortNoteComplete": {
      "type": "boolean",
      "default": false
    },
    "gender": {
      "type": "enumeration",
      "enum": [
        "male",
        "female"
      ],
      "required": true
    },
    "language": {
      "type": "enumeration",
      "enum": [
        "English",
        "Bangla"
      ],
      "required": true,
      "default": "English"
    },
    "currentDay": {
      "type": "integer",
      "default": 1,
      "unique": false
    }
  }
}
